<% include ../partial/atas %>
<script src="/dist/js/jquery.min.js"></script>
<script src="/dist/js/jquery.dataTables.min.js"></script>


<script src="/adapter/tbl.js"></script>

<% var arrdata = ["CLOSE","OPEN"]; %>
<div class="row">
<div class="col-xs-12">
<div class="box box-primary">
<div class="box-header with-border">
      <table id="Table" class="display">
      <thead>
      	<tr>
      		 <th>ID</th>
           <th>SITE</th>
           <th>TANGGAL</th>
           <th>TEMP</th>
           <th>DOOR</th>
           <th>AC/PWR</th>
           <th>FUEL</th>
                  
        </tr>
      
      </thead>
       <tbody><% for (var i=0;i < log.length; i++){   %>
        <tr>
        <td><%= log[i].id %></td>
        <td><%= log[i].site %></td>
        <td><%= log[i].tanggal %></td>
        <td><%= log[i].temp %></td>
        <td><%= arrdata[log[i].door] %></td>
        <td><%= log[i].acpwr %></td>
        <td><%= log[i].FUEL %></td>
          <% } %>
        </tr>
      </tbody>
  	  </table>
</div>
</div>
</div>
</div>
</div>

 




